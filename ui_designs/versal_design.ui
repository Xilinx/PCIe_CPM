}

/* The actual progress bar */
.progress-bar {
  width: 0%;
  height: 100%;
  background-color: #4CAF50; /* Green color */
  border-radius: 4px;
  transition: width 0.3s ease;
}

/* Style for the progress bar text */
.progress-text {
  color: black;
  text-align: center;
  line-height: 20px;
}

/* Styling when the progress bar is complete */
.progress-bar.complete {
  background-color: #008000; /* Dark green color */
}

/* Styling when the progress bar is in progress */
.progress-bar.in-progress {
  background-color: #FFA500; /* Orange color */
}

/* Styling when the progress bar is not started */
.progress-bar.not-started {
  background-color: #808080; /* Gray color */
}</string>
